{ 
  "listenAddress": ":8080",
  "accounts": {
    "spinup": {
      "region": "us-east-1",
      "akid": "{{ .spinup_akid }}",
      "secret": "{{ .spinup_secret }}"
    },
    "spinupsec": {
      "region": "us-east-1",
      "akid": "{{ .spinupsec_akid }}",
      "secret": "{{ .spinupsec_secret }}"
    }
  },
  "account": {
    "region": "us-east-1",
    "akid": "{{ .akid }}",
    "secret": "{{ .secret }}",
    "externalId": "{{ .external_id }}",
    "role": "{{ .cross_account_role }}"
  },
  "imageCache": {
    "region": "us-east-1",
    "bucket": "{{ .imagecache_bucket }}",
    "prefix": "{{ .imagecache_bucket_prefix }}",
    "akid": "{{ .imagecache_akid }}",
    "secret": "{{ .imagecache_secret }}",
    "hashingToken": "{{ .imagecache_hashing_token }}"
  },
  "token": "{{ .api_token }}",
  "logLevel": "{{ .log_level }}",
  "org": "{{ .spinup_org }}",
  "cacheExpireTime": "{{ .cache_expire_time }}",
  "cachePurgeTime": "{{ .cache_purge_time }}"
}
